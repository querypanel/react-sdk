{
  "name": "@querypanel/react-sdk",
  "version": "0.1.2",
  "description": "React components for QueryPanel - AI-powered data visualization",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./themes": {
      "types": "./dist/themes/index.d.ts",
      "import": "./dist/themes/index.mjs",
      "require": "./dist/themes/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook-static"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@blocknote/core": "^0.37.0",
    "@blocknote/mantine": "^0.37.0",
    "@blocknote/react": "^0.37.0",
    "lucide-react": "^0.511.0",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^8.4.0",
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-interactions": "^8.4.0",
    "@storybook/blocks": "^8.4.0",
    "@storybook/manager-api": "^8.4.0",
    "@storybook/react": "^8.4.0",
    "@storybook/react-vite": "^8.4.0",
    "@storybook/theming": "^8.4.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5",
    "typescript-eslint": "^8.49.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "react",
    "querypanel",
    "data-visualization",
    "vizspec",
    "recharts",
    "charts"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/querypanel/react-sdk"
  }
}
